{
  "name": "comicwise",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "// Development Scripts": "",
    "clean": "rimraf .next out dist build .turbo coverage",
    "predev": "pnpm clean",
    "dev": "next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "// Build Scripts": "",
    "prebuild": "pnpm clean && pnpm type-check",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:debug": "next build --debug",
    "build:profile": "next build --profile",
    "postbuild": "next-sitemap",
    "// Production Scripts": "",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "preview": "pnpm build && pnpm start",
    "// Code Quality Scripts": "",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --fix-type problem --fix-type suggestion --fix-type layout",
    "lint:strict": "eslint . --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "pnpm type-check && pnpm lint && pnpm format:check",
    "// Spell Checking": "",
    "cspell": "cspell \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "cspell:fix": "cspell \"**/*.{ts,tsx,js,jsx,json,md}\" --fix",
    "// Database Scripts": "",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:seed": "tsx --env-file=.env.local src/database/seed/index.ts",
    "db:seed:users": "tsx --env-file=.env.local src/database/seed/index.ts --users-only",
    "db:seed:comics": "tsx --env-file=.env.local src/database/seed/index.ts --comics-only",
    "db:seed:chapters": "tsx --env-file=.env.local src/database/seed/index.ts --chapters-only",
    "db:seed:dry-run": "tsx --env-file=.env.local src/database/seed/index.ts --dry-run",
    "db:seed:verbose": "tsx --env-file=.env.local src/database/seed/index.ts --verbose",
    "db:seed:no-images": "tsx --env-file=.env.local src/database/seed/index.ts --skip-images",
    "db:reset": "pnpm db:push && pnpm db:seed",
    "db:backup": "pg_dump $DATABASE_URL > backup-$(date +%Y%m%d-%H%M%S).sql",
    "// Testing Scripts": "",
    "test": "pnpm exec playwright test",
    "test:ui": "pnpm exec playwright test --ui",
    "test:headed": "pnpm exec playwright test --headed",
    "test:debug": "pnpm exec playwright test --debug",
    "test:codegen": "pnpm exec playwright codegen",
    "test:report": "pnpm exec playwright show-report",
    "test:chromium": "pnpm exec playwright test --project=chromium",
    "test:firefox": "pnpm exec playwright test --project=firefox",
    "test:webkit": "pnpm exec playwright test --project=webkit",
    "test:mobile": "pnpm exec playwright test --project='Mobile Chrome'",
    "test:example": "pnpm exec playwright test example",
    "test:trace": "pnpm exec playwright test --trace on",
    "test:update-snapshots": "pnpm exec playwright test --update-snapshots",
    "test:auth": "pnpm exec playwright test auth.spec.ts",
    "test:crud": "pnpm exec playwright test crud.spec.ts",
    "test:ci": "pnpm exec playwright test --reporter=github",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:run": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "// Docker Scripts": "",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:prune": "docker system prune -af --volumes",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:test": "bash test-docker.sh",
    "// Dependency Management": "",
    "check-updates": "pnpm outdated",
    "check-updates:ncu": "ncu -u --deep --packageManager pnpm",
    "update-deps": "pnpm update --interactive --latest",
    "update-deps:patch": "pnpm update --latest",
    "update-deps:minor": "pnpm update --latest -r",
    "update:deps": "pnpm npm-check-updates -u && pnpm install",
    "prepare": "husky",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "dedupe": "pnpm dedupe",
    "// Setup & Quickstart": "",
    "quickstart": "pwsh quickstart.ps1",
    "setup": "pnpm install && pnpm db:push && pnpm db:seed",
    "setup:clean": "pnpm clean && pnpm install && pnpm db:reset",
    "// CI/CD Scripts": "",
    "ci": "pnpm validate && pnpm test",
    "ci:test": "pnpm test --reporter=json --reporter=html",
    "ci:build": "pnpm build",
    "// Deployment Scripts": "",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:docker": "docker-compose -f docker-compose.yml up -d --build",
    "// Utility Scripts": "",
    "info": "next info",
    "telemetry:disable": "next telemetry disable",
    "telemetry:enable": "next telemetry enable",
    "analyze-bundle": "ANALYZE=true pnpm build",
    "find-deadcode": "ts-prune",
    "// Performance & Profiling": "",
    "lighthouse": "lighthouse http://localhost:3000 --view",
    "bundle-size": "pnpm build && size-limit",
    "// Email Testing": "",
    "email:dev": "email dev --dir src/components/emails --port 3001",
    "email:export": "email export --dir src/components/emails --out .react-email",
    "stub": "ts-stub --clear",
    "types:generate": "tsx scripts/generate-stub-types.ts",
    "types:install": "echo 'Run pnpm install @types/...' as needed"
  },
